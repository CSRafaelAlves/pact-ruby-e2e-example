{
  "consumer": {
    "name": "Foo"
  },
  "provider": {
    "name": "Bar"
  },
  "interactions": [
    {
      "description": "a retrieve thing request",
      "request": {
        "method": "get",
        "path": "/foo",
        "headers": {
          "Accept": "application/json"
        }
      },
      "response": {
        "status": 200,
        "headers": {
          "Content-Type": "application/json"
        },
        "body": {
          "results": [
            {
              "name": "Games"
            }
          ]
        },
        "matchingRules": {
          "$.body.results": {
            "min": 1
          },
          "$.body.results[*].*": {
            "match": "type"
          },
          "$.body.results[*].name": {
            "match": "regex",
            "regex": "Games|Book Clubs"
          }
        }
      }
    }
  ],
  "metadata": {
    "pactSpecification": {
      "version": "2.0.0"
    }
  }
}